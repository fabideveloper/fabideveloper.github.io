    {
      "title": "A simple upgrade system using the BigNum Package",
      "language": "Luau",
      "description": "Upgrades certain properties simply, and manages big numbers over the engine's limits using the BigNum Module.",
      "snippet": [
"function UpgradesService:RequestUpgrades(Player : Player, WhatExactly : string?, ...) : any",
"\tlocal PlayerUpgrades = self.CachedServices.PlayersService:RequestData(Player, \"Upgrades\")",
"",
"\tif WhatExactly == \"UpgradeNextPrices\" then",
"\t\tlocal args = ...",
"\t\tlocal data = {}",
"",
"\t\tfor upgradeName, info in Upgrades_Data do",
"\t\t\tlocal level = PlayerUpgrades[upgradeName] or 0",
"\t\t\tlocal upgradeNameVal = BigNum.mt.tostring(BigNum.mt.mul(info.Base, (info.Multiplier ^ level) * info.Increment))",
"\t\t\tdata[upgradeName] = upgradeNameVal",
"\t\tend",
"",
"\t\treturn args ~= nil and data[args] or data, PlayerUpgrades, args ~= nil and Upgrades_Data[args] or Upgrades_Data",
"\tend",
"",
"\treturn {}, PlayerUpgrades, Upgrades_Data",
"end",
"",
"function UpgradesService.Client:RequestUpgrades(...)",
"\treturn self.Server:RequestUpgrades(...)",
"end"
]
